#include "stdafx.h"	
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{
 int numero1,numero2,res;
 cout<<"ingresar numero1";
 cin>>numero1;
 cout<<"ingresar numero2";
 cin>>numero2;
	 
 if (numero1%2==0&&numero2%2==0)
{
 res=numero1+numero2;
}
else
{
	res=numero1*numero2;
}
cout<<res;
getch();
}
